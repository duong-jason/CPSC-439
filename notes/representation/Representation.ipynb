    "def NtS(n):# natural numbers to strings\n",
    "    if n > 1:\n",
    "        return NtS(n // 2) + str(n % 2)\n",
    "    else:\n",
    "        return str(n % 2)\n",
    "    \n",
    "NtS(344)"
   ]
  },
